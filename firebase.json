{
  "hosting": {
    "public": "dist/test-i18n/en-US",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/en{,/**}",
        "destination": "/en/index.html"
      },
      {
        "source": "/it{,/**}"
        ,
        "destination": "/it/index.html"
      }
    ]
  }
}
